{
    "name": "Youtube Resizer",
    "description": "Makes youtube video fullscreen and can enlarge further",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "scripting", "activeTab", "tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/size.png",
            "32": "/images/size_32.png",
            "64": "/images/size_64.png",
            "128": "/images/size_128.png",
            "256": "/images/size_256.png",
            "512": "/images/size_512.png"
        }
    },
    "icons": {
        "16": "/images/size.png",
        "32": "/images/size_32.png",
        "64": "/images/size_64.png",
        "128": "/images/size_128.png",
        "256": "/images/size_256.png",
        "512": "/images/size_512.png"
    }
}